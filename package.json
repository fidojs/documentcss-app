{
  "name": "documentCSS",
  "version": "0.2.0",
  "description": "Electron application for <document/>CSS",
  "keywords": [
    "Electron",
    "demo",
    "quick",
    "start",
    "tutorial"
  ],
  "license": "MIT",
  "author": "Bitovi",
  "main": "main.js",
  "repository": "https://github.com/documentcss/documentcss-app",
  "scripts": {
    "dist": "electron-builder",
    "inspect": "electron --debug-brk --inspect .",
    "pack": "electron-builder --dir",
    "start": "electron ."
  },
  "dependencies": {
    "@fidojs/fidojs-kennel-console": "^0.2.6",
    "electron-prompt": "^0.3.0",
    "electron-updater": "^2.8.7",
    "express": "^4.15.4",
    "fix-path": "^2.1.0",
    "fs-extra": "^4.0.1",
    "glitch-please": "^0.2.7",
    "portfinder": "^1.0.13"
  },
  "devDependencies": {
    "electron": "~1.6.2",
    "electron-builder": "^19.24.1"
  },
  "build": {
    "appId": "com.github.fidojs.documentcssapp",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "publish": [
      {
        "provider": "github",
        "owner": "fidojs",
        "repo": "documentcss-app"
      }
    ],
    "directories": {
      "buildResources": "build-resources"
    },
    "asar": true
  }
}
